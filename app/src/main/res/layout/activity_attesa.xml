<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sfondo3"
    tools:context=".activity_attesa">

    <ProgressBar
        android:id="@+id/pbAttesa"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="152dp"
        android:layout_height="171dp"
        android:layout_centerHorizontal="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.164" />

    <Button
        android:id="@+id/btnAnnulla"
        android:layout_width="117dp"
        android:layout_height="58dp"
        android:backgroundTint="#2CA05A"
        android:onClick="onClickAnnulla"
        android:text="Annulla"
        android:textColor="#212121"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pbAttesa" />

</androidx.constraintlayout.widget.ConstraintLayout>